<!-- cell_border.xml -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <solid android:color="#FFFFFF"/> <!-- 单元格背景色，白色 -->
    <stroke android:color="#000000" android:width="1dp"/> <!-- 边框颜色为黑色，宽度为1dp -->
    <corners android:radius="4dp"/> <!-- 边框圆角半径为4dp -->
</shape>